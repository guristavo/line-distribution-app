{
  "name": "line-distribution-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://kavispires.github.io/line-distribution-app/",
  "dependencies": {
    "firebase": "^5.5.7",
    "http-status-codes": "^1.3.0",
    "informed": "^2.0.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^5.1.0",
    "react-redux-toastr": "^7.4.1",
